{
  "id": "flooring-installation-service",
  "stylesheets": [
    {
      "content": {
        "#flooring-installation-service": {
          "ul": {
            "height": "50vh",
            "maxHeight": "500px",
            "padding": 0,
            "margin": 0,
            "listStyle": "none",
            "justifyContent": "space-between",
            "gap": "20px",
            "li": {
              "flex": 1,
              "textAlign": "center",
              "height": "100%",
              "max-width": "calc(100% / 7)",
              "opacity": 0,
              "animation": "slide-up 0.7s ease forwards",
              "img": {
                "flex": 1
              }
            }
          }
        }
      }
    }
  ],
  "content": [
    {
      "name": "section",
      "id": "flooring-installation-service",
      "class": "center col view-section service-section",
      "children": [
        {
          "name": "div",
          "class": "wrapper",
          "children": [
            {
              "name": "header",
              "children": [
                {
                  "name": "button",
                  "class": "return-btn",
                  "children": [
                    {
                      "name": "img",
                      "src": "{{env.assetsOrigin}}assets/icons/return-arrow.icon.svg"
                    }
                  ]
                },
                {
                  "name": "h3",
                  "children": [
                    "flooring Installation"
                  ]
                },
                {
                  "name": "p",
                  "children": [
                    "Professional installation of any kind, all dustless sanding which means we are in and out without any mess."
                  ]
                }
              ]
            },
            {
              "name": "ul",
              "class": "row",
              "children": [
                {
                  "name": "li",
                  "class": "col",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/tile-sample.webp" },
                    { "name": "span", "children": ["Tile"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.1s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/hardwood-sample.webp" },
                    { "name": "span", "children": ["Hardwood"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.2s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/vinyl-sample.webp" },
                    { "name": "span", "children": ["Vinyl"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.3s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/epoxy-sample.webp" },
                    { "name": "span", "children": ["Epoxy"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.4s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/carpet-sample.webp" },
                    { "name": "span", "children": ["Carpet"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.5s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/rubber-sample.webp" },
                    { "name": "span", "children": ["Rubber"] }
                  ]
                },
                {
                  "name": "li",
                  "class": "col",
                  "style": "animation-delay: 0.6s",
                  "children": [
                    { "name": "img", "src": "{{env.assetsOrigin}}assets/concrete-sample.webp" },
                    { "name": "span", "children": ["Concrete"] }
                  ]
                }
              ]
            }
          ]
        }
      ]
    }
  ]
}
